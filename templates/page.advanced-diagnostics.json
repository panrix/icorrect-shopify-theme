    "custom_liquid_NQ4xni": {
      "type": "custom-liquid",
      "name": "t:sections.custom-liquid.presets.name",
      "settings": {
        "custom_liquid": "<style>\n  .diagnostic-section {\n    background: #fafafa;\n    padding: 80px 40px;\n    font-family: -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, Inter, sans-serif;\n  }\n\n  .diagnostic-header {\n    text-align: center;\n    max-width: 1000px;\n    margin: 0 auto 70px auto;\n  }\n\n  .diagnostic-header h2 {\n    font-size: 40px;\n    font-weight: 700;\n    margin-bottom: 18px;\n    color: #111;\n  }\n\n  .diagnostic-header p {\n    font-size: 18px;\n    line-height: 1.65;\n    color: #444;\n  }\n\n  .diagnostic-grid {\n    display: grid;\n    grid-template-columns: repeat(5, 1fr);\n    gap: 32px;\n    max-width: 1600px;\n    margin: 0 auto;\n  }\n\n  .diagnostic-card {\n    background: #ffffff;\n    border-radius: 20px;\n    padding: 38px 34px 42px;\n    box-shadow: 0 12px 30px rgba(0,0,0,0.08);\n    display: flex;\n    flex-direction: column;\n    height: 100%;\n  }\n\n  .diagnostic-card img {\n    width: 52px;\n    height: 52px;\n    margin-bottom: 26px;\n  }\n\n  .diagnostic-card h3 {\n    font-size: 18px;\n    font-weight: 700;\n    margin-bottom: 16px;\n    color: #111;\n    line-height: 1.3;\n  }\n\n  .diagnostic-card p {\n    font-size: 16px;\n    line-height: 1.6;\n    color: #444;\n    margin: 0;\n  }\n\n  @media (max-width: 1400px) {\n    .diagnostic-grid {\n      grid-template-columns: repeat(2, 1fr);\n    }\n  }\n\n  @media (max-width: 768px) {\n    .diagnostic-grid {\n      grid-template-columns: 1fr;\n    }\n  }\n</style>\n\n<div class=\"diagnostic-section\">\n  <div class=\"diagnostic-header\">\n    <h2>What We Test and How We Do It</h2>\n    <p>\n      We begin by evaluating your device externally, testing voltage and current draw using precision power meters.\n      These tell us if the device is accepting power correctly or if there is a fault in the charging circuit.\n      Then we move to deeper, internal analysis.\n    </p>\n  </div>\n\n  <div class=\"diagnostic-grid\">\n    <div class=\"diagnostic-card\">\n      <img src=\"https://cdn-icons-png.flaticon.com/512/6916/6916653.png\">\n      <h3>Step 1: External Power Testing</h3>\n      <p>\n        We check how much current your device is drawing from the charger.\n        This shows if the power path is working or where power loss occurs.\n      </p>\n    </div>\n\n    <div class=\"diagnostic-card\">\n      <img src=\"https://cdn-icons-png.flaticon.com/512/4102/4102763.png\">\n      <h3>Step 2: DC Power Supply Injection</h3>\n      <p>\n        We apply power directly to the logic board to simulate boot conditions\n        and understand board behaviour under controlled conditions.\n      </p>\n    </div>\n\n    <div class=\"diagnostic-card\">\n      <img src=\"https://cdn-icons-png.flaticon.com/512/4245/4245134.png\">\n      <h3>Step 3: Thermal & Visual Inspection</h3>\n      <p>\n        Using microscopes and thermal imaging, we identify shorts,\n        corrosion, or failed components. Everything is documented and traceable.\n      </p>\n    </div>\n\n    <div class=\"diagnostic-card\">\n      <img src=\"https://cdn-icons-png.flaticon.com/512/1423/1423903.png\">\n      <h3>Step 4: Logic Board Circuit Mapping</h3>\n      <p>\n        We test power, charging and audio circuits using custom repair maps\n        and multimeters to locate the fault precisely.\n      </p>\n    </div>\n\n    <div class=\"diagnostic-card\">\n      <img src=\"https://cdn-icons-png.flaticon.com/512/4388/4388366.png\">\n      <h3>Step 5: Quote and Report</h3>\n      <p>\n        We send a report including the issue found, relevant photos,\n        and a detailed quote. You stay informed every step of the way.\n      </p>\n    </div>\n  </div>\n</div>",
        "color_scheme": "",
        "padding_top": 0,
        "padding_bottom": 0
      }
    },
    "custom_liquid_bREMwK": {
